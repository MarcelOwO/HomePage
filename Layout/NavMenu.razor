<MudPaper Width="200" Class="pa-3 ma-3" Elevation="23">
    <MudNavMenu>
        <MudNavLink Icon="@Icons.Material.Filled.Home" Href="/">
            <MudText Align="Align.Center">Home</MudText>
        </MudNavLink>
        <MudNavLink Icon="@Icons.Material.Filled.Person" Href="/WorkExperience">
            <MudText Align="Align.Center">Work Experience</MudText>
        </MudNavLink>
        <MudNavLink Icon="@Icons.Material.Filled.Code" Href="/Projects">
            <MudText Align="Align.Center">Projects</MudText>
        </MudNavLink>
        <MudNavLink Icon="@Icons.Material.Filled.Camera" Href="/Photography">
            <MudText Align="Align.Center">Photography</MudText>
        </MudNavLink>
      
    </MudNavMenu>
</MudPaper>

<MudPaper Width="200" Class="pa-3 ma-3" Elevation="23">
    <MudStack>
        @foreach (var contact in _contacts)
        {
            <MudButton Href=@contact.Value.Item1 StartIcon=@contact.Value.Item2>
                <MudText>@contact.Key</MudText>
            </MudButton>
        }
    </MudStack>
</MudPaper>

@code {
    private readonly Dictionary<string, (string, string)> _contacts = new()
    {
        ["Twitter"] = ("https://x.com/Marcel_Sigl", @Icons.Custom.Brands.Twitter),
        ["GitHub"] = ("https://github.com/MarcelOwO", @Icons.Custom.Brands.GitHub),
        ["Instagram"] = ("https://www.instagram.com/marcel_sigl/ ", @Icons.Custom.Brands.Instagram),
        ["Steam"] = ("https://steamcommunity.com/id/Marcelowo", @Icons.Custom.Brands.Steam),
    };
}