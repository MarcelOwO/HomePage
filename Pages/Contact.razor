@page "/Contact"
<MudContainer Class="mt-16 px-8" MaxWidth="MaxWidth.False">
    <MudPaper Class="pa-16 ma-2" Elevation="3">
        <MudGrid Justify="Justify.Center">
            <MudItem xs="3">
                <MudPaper Class="d-flex allign-center" Width="300" Height="300" Elevation="0">
                    <MudIconButton Allign="Allign.Center" Href="https://github.com/MarcelOwO" Icon="@Icons.Custom.Brands.GitHub" Size="Size.Large"/>
                    <MudText Align="Align.Center" Typo="Typo.h5">GitHub</MudText>

                </MudPaper>
            </MudItem>

            <MudItem xs="3">
                <MudPaper Class="d-flex allign-center" Width="300" Height="300" Elevation="0">
                    <MudIconButton Href="https://steamcommunity.com/id/Marcelowo" Icon="@Icons.Custom.Brands.Steam" Size="Size.Large"/>
                    <MudText Align="Align.Center" Typo="Typo.h5">Steam</MudText>
                </MudPaper>
            </MudItem>

            <MudItem xs="3">
                <MudPaper Class="d-flex allign-center" Width="300" Height="300" Elevation="0">
                    <MudIconButton Href="https://www.instagram.com/marcel_sigl/" Icon="@Icons.Custom.Brands.Instagram" Size="Size.Large"/>
                    <MudText Align="Align.Center" Typo="Typo.h5">Instagram</MudText>
                </MudPaper>
            </MudItem>

            <MudItem xs="3">
                <MudPaper Class="d-flex allign-center" Width="300" Height="300" Elevation="0">
                    <MudIconButton Href="https://x.com/Marcel_Sigl" Icon="@Icons.Custom.Brands.Twitter" Size="Size.Large"/>
                    <MudText Align="Align.Center" Typo="Typo.h5">Twitter</MudText>
                </MudPaper>
            </MudItem>
        </MudGrid>
    </MudPaper>
</MudContainer>

